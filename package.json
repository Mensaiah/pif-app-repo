{
  "name": "pif-api",
  "version": "1.0.0",
  "description": "PIF API built with Node.js and Express",
  "main": "src/app.ts",
  "scripts": {
    "start": "node bin/prod/www",
    "start-mongo": "./start-mongo.sh",
    "dev": "concurrently \"pnpm run start-mongo\" \"ts-node-dev -r tsconfig-paths/register --respawn --exit-child --clear bin/dev/www\" ",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "test": "jest src --detectOpenHandles",
    "docker:build": "docker build -t pif-api-img .",
    "docker:run": "docker run -d -p 3000:3000 --name pif-container pif-api-img",
    "docker:dev": "docker-compose up --build",
    "start:redis": "docker run -p 6379:6379 -it redis/redis-stack-server:latest",
    "check-unused": "npx depcheck",
    "do:migration": "NODE_ENV=testing ts-node-dev src/migration/",
    "migration:lab": "ts-node-dev src/migration/testLab.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@types/bcrypt": "^5.0.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.1.0",
    "bson": "^5.2.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dompurify": "^3.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase": "^9.21.0",
    "firebase-admin": "^11.7.0",
    "geoip-lite": "^1.4.7",
    "helmet": "^6.1.5",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.0",
    "mailgun.js": "^8.2.1",
    "mime": "^3.0.0",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "mongoose": "^7.1.0",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.4.5",
    "node-fetch": "^3.3.1",
    "qs": "^6.11.2",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.6",
    "ssh2": "^1.14.0",
    "stripe": "^12.13.0",
    "swagger-ui-express": "^4.6.3",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dompurify": "^3.0.2",
    "@types/express": "^4.17.17",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^29.5.1",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mime": "^3.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.16.2",
    "@types/qs": "^6.9.7",
    "@types/ssh2": "^1.11.12",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "form-data": "^4.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pif-org/pif-api.git"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "keywords": [
    "PIF",
    "Gift",
    "Charity",
    "API",
    "Node.js",
    "Express"
  ],
  "license": "ISC",
  "exports": {
    "./dist/node/axios.cjs": "./dist/node/axios.cjs"
  }
}
